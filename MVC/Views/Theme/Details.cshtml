@model Domain.ProjectLogics.MainTheme

@{
    Layout = "_Layout";
    ViewData["Title"] = "Main Themes";
}

<div>
    <h1>Theme <span id="themeId">@Model.Id</span>: @Model.Subject</h1>
    <div>
        <table class="table">
            <thead>
            <tr>
                <th>Flow ID</th>
            </tr>
            </thead>
            <tbody id="flowContainer">

            </tbody>
        </table>
    </div>
    <div>
        <table class="table">
            <thead>
            <tr>
                <th>Subtheme ID</th><th>Subject</th><th>Details</th>
            </tr>
            </thead>
            <tbody id="subthemeContainer">
            </tbody>
        </table>
    </div>
</div>


<script src="/dist/theme.entry.js" defer></script>
